class Recipe(recipe.Recipe):
    name = 'mpfr'
    version = '4.0.1'
    stype = SourceType.TARBALL
    url = 'https://ftpmirror.gnu.org/gnu/mpfr/mpfr-%(version)s.tar.bz2'
    licenses = [License.LGPLv3Plus]
    configure_options = "--disable-shared --enable-static"
    deps = ['gmp']
